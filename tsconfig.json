{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strictNullChecks": true,
    "removeComments": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ]
    }
  }
}
